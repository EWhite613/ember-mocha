{
  "name": "ember-mocha",
  "version": "0.0.1",
  "description": "Mocha helpers for testing Ember.js applications",
  "main": "lib/ember-mocha.js",
  "scripts": {
    "build": "rm -rf build && BROCCOLI_ENV=test broccoli build build",
    "prepublish": "bower install",
    "pretest": "npm run-script build",
    "test": "testem ci",
    "start": "BROCCOLI_ENV=test broccoli serve"
  },
  "authors": [
    "Dan Gebhardt"
  ],
  "license": "Apache Version 2.0",
  "devDependencies": {
    "bower": "^1.3.12",
    "broccoli": "^0.13.0",
    "broccoli-concat": "^0.0.11",
    "broccoli-es6-concatenator": "^0.1.7",
    "broccoli-merge-trees": "^0.1.4",
    "broccoli-static-compiler": "^0.1.4",
    "testem": "0.6.19"
  }
}
